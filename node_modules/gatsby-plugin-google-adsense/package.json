{
  "name": "gatsby-plugin-google-adsense",
  "version": "1.1.3",
  "description": "Gatsby plugin to add Google Adsense auto ads to a website",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/callicoder/gatsby-plugin-google-adsense.git"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "google adsense",
    "auto ads"
  ],
  "author": "Rajeev Singh <rajeevhub@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/callicoder/gatsby-plugin-google-adsense.git/issues"
  },
  "homepage": "https://github.com/callicoder/gatsby-plugin-google-adsense.git#readme",
  "dependencies": {
    "@babel/runtime": "^7.2.0"
  },
  "peerDependencies": {
    "gatsby": ">2.0.0-alpha"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "babel-preset-gatsby-package": "^0.1.3",
    "cross-env": "^5.2.0"
  }
}
